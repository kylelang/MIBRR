#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
  vector<int> x = {1, 2, 3, 4, 5};
  vector<int>::iterator it;
  for(it = x.begin(); it != x.end(); ++it) {
    cout << "Here's your vector, buddy:\n" << endl;
    cout << *it << " " << endl;
  }
  return 0;
}
